{
  "name": "@ynok/core",
  "version": "0.0.1",
  "description": "ynok core",
  "main": "dist/src/main.js",
  "types": "dist/src/main.d.ts",
  "scripts": {
    "tsc:dev": "tsc --declaration --emitDeclarationOnly --outDir dist --watch",
    "swc:dev": "swc src -d dist --config-file .swcrc --watch",
    "client:dev": "rsbuild dev",
    "dev": "concurrently \"npm run swc:dev\" \"npm run tsc:dev\" \"npm run client:dev\"",
    "tsc:build": "tsc --build --declaration --emitDeclarationOnly --verbose",
    "swc:build": "swc src -d dist --config-file .swcrc",
    "client:build": "rsbuild build",
    "build": "npm run swc:build && npm run tsc:build && npm run client:build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tori-getz/ynok.git"
  },
  "keywords": [
    "ssr"
  ],
  "author": "Daniil Benger (tori-getz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tori-getz/ynok/issues"
  },
  "homepage": "https://github.com/tori-getz/ynok#readme",
  "devDependencies": {
    "@rsbuild/core": "^1.1.13",
    "@swc/cli": "^0.5.2",
    "@swc/core": "^1.10.4",
    "@types/express": "^5.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/uuid": "^10.0.0",
    "@types/ws": "^8.5.13",
    "chokidar": "^3.6.0",
    "jsdom": "^25.0.1",
    "typescript": "^5.7.2"
  },
  "dependencies": {
    "express": "^4.21.2",
    "puppeteer": "^23.11.1",
    "ts-pattern": "^5.6.0",
    "uuid": "^11.0.3",
    "ws": "^8.18.0"
  }
}
