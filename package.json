{
  "name": "ynok",
  "version": "0.0.1",
  "description": "SSR Framework",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "lint-staged": "lint-staged",
    "format": "prettier . --write",
    "commitlint": "commitlint --edit",
    "prepare": "husky",
    "dev": "npm run dev --workspaces",
    "build": "npm run build --workspaces"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tori-getz/ynok.git"
  },
  "keywords": [
    "ssr"
  ],
  "author": "Daniil Benger (tori-getz)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tori-getz/ynok/issues"
  },
  "homepage": "https://github.com/tori-getz/ynok#readme",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,md}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "concurrently": "^9.1.2",
    "husky": "^9.1.7",
    "lint-staged": "^15.3.0",
    "nodemon": "^3.1.9",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.4.2"
  },
  "packageManager": "yarn@1.22.19+sha1.4ba7fc5c6e704fce2066ecbfb0b0d8976fe62447"
}
